{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "Price": {
      "type": "decimal"
    },
    "stock": {
      "type": "integer"
    },
    "stat": {
      "type": "enumeration",
      "enum": [
        "available",
        "unavailable",
        "coming soon"
      ]
    },
    "Image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "string"
    }
  }
}
